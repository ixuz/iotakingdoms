    "https://linkedsoftwaredependencies.org/bundles/npm/iotakingdoms/^0.0.1/config/": "config/",
    "https://linkedsoftwaredependencies.org/bundles/npm/iotakingdoms/^0.0.1/dist/": "dist/"
  },
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
